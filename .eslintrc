{
	"env": {
		"browser": true,
		"es2021": true
	},
	"extends": ["plugin:@wordpress/eslint-plugin/recommended", "prettier"],
	"plugins": ["prettier"],
	"overrides": [],
	"parserOptions": {
		"ecmaVersion": "latest",
		"sourceType": "module"
	},
	"rules": {
		"max-len": [
			"warn",
			{
				"code": 120
			}
		],
		"prettier/prettier": "error"
	}
}
